  {
    "@type": "Webpage",
    "url": "https://support.mozilla.org/{{ LANG }}/products/mobile"
  },
  "releaseNotes": "{% filter absolute_url %}{{ url('firefox.notes', platform='android') }}{% endfilter %}",
  "isPartOf":
  {
    "@id": "{{ structured_data_id('firefoxbrowser') }}",
